#define ALL_MUX_CH_1() XGpio_DiscreteWrite(&Gpio1,1,0x88888888);XGpio_DiscreteWrite(&Gpio1,2,0x88);
#define ALL_MUX_CH_2() XGpio_DiscreteWrite(&Gpio1,1,0x99999999);XGpio_DiscreteWrite(&Gpio1,2,0x99);
